	Keywords = {Gridding, algorithm, MRI, reconstruction},
	Month = {Aug},
	Pmid = {22898698},
	Pst = {aheadofprint},
	Rating = {1},
	Read = {1},
	Title = {A CUDA-based reverse gridding algorithm for MR reconstruction},
