	Doi = {10.1152/ajpheart.00802.2003},
	Journal = {Am J Physiol Heart Circ Physiol},
	Journal-Full = {American journal of physiology. Heart and circulatory physiology},
	Mesh = {Animals; Anoxia; Chronic Disease; Diastole; Echocardiography; Hypertension, Pulmonary; Male; Myocardium; Rats; Rats, Wistar; Systole; Ventricular Dysfunction, Right},
	Month = {Sep},
	Number = {3},
