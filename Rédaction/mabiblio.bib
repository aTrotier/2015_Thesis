	Keywords = {cardiovascular; contrast enhancement; micro-CT; mouse; perfusion},
	Month = {Apr},
	Pmid = {24764151},
	Pst = {aheadofprint},
	Title = {Three-dimensional imaging of the mouse heart and vasculature using micro-CT and whole-body perfusion of iodine or phosphotungstic acid},
