@article{Ersoy:2004aa,
	Abstract = {OBJECTIVE: The purpose of our research was to investigate the value of a blood pool contrast agent in detecting endoleaks on MR angiography after endoluminal stent-graft repair of infrarenal aortic aneurysms.
