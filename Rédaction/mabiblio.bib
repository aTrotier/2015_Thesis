@article{La-Roque:2011rw,
	Abstract = {Pulmonary arterial hypertension (PAH) is a severe disease that leads to increased pulmonary vascular resistance and right heart failure. Noninvasive methods are needed to detect changes in the pulmonary artery circulation during PAH establishment and/or treatment. Pulmonary blood flow velocity can be evaluated by dynamic MR angiography, although the relevance of such data in the context of PAH remains to be demonstrated. A novel dynamic MR angiography technique was used in this work to measure blood flow velocity in the pulmonary arteries of the same living animals, before and after the establishment of chronic hypoxia-induced PAH. Chronic hypoxia decreased significantly the blood flow velocity (43.8 $\pm$ 4.9 vs 24.3 $\pm$ 8.7  cm/s) on electrocardiography-triggered time-resolved angiograms. In parallel, chronic hypoxia-induced PAH was confirmed from invasive measurements of the mean pulmonary arterial pressure (32.1 $\pm$ 4.8 vs 12.5 $\pm$ 2.2 mmHg) and the ratio of the right ventricle weight to the left ventricle plus septum weight (Fulton index: 0.54 $\pm$ 0.06 vs 0.27 $\pm$ 0.04). This study demonstrates the potential interest of dynamic MR angiography for the investigation of experimental models and for the evaluation of treatment efficacy.},
	Author = {de La Roque, Eric Dumas and Thiaudi{\`e}re, Eric and Ducret, Thomas and Marthan, Roger and Franconi, Jean-Michel and Guibert, Christelle and Parzy, Elodie},
	Date-Added = {2013-10-24 16:20:14 +0200},
