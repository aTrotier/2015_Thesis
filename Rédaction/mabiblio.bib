	Doi = {10.1002/jmri.22437},
	Journal = {J Magn Reson Imaging},
	Journal-Full = {Journal of magnetic resonance imaging : JMRI},
	Mesh = {Algorithms; Blood Flow Velocity; Cerebral Arteries; Cerebrovascular Circulation; Echo-Planar Imaging; Female; Humans; Image Enhancement; Image Interpretation, Computer-Assisted; Imaging, Three-Dimensional; Male; Reproducibility of Results; Sensitivity and Specificity; Spin Labels; Young Adult},
