	Pmid = {20190209},
	Pst = {ppublish},
	Title = {Contrast-enhanced MR imaging in acute ischemic stroke: T2* measures of blood-brain barrier permeability and their relationship to T1 estimates and hemorrhagic transformation},
	Volume = {31},
