	Mesh = {Angiography, Digital Subtraction; Brain; Cerebral Angiography; Cerebrovascular Disorders; Forecasting; Humans; Imaging, Three-Dimensional; Magnetic Resonance Angiography},
	Month = {Dec},
	Number = {6},
