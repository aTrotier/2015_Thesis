	Doi = {10.1002/mrm.24463},
	Journal = {Magn Reson Med},
	Journal-Full = {Magnetic resonance in medicine : official journal of the Society of Magnetic Resonance in Medicine / Society of Magnetic Resonance in Medicine},
	Keywords = {compressed sensing; dynamic cardiac MRI; motion correction; nonrigid motion; undersampling},
	Month = {Aug},
	Number = {2},
	Pages = {504-16},
	Pmid = {22899104},
	Pst = {ppublish},
	Title = {Motion corrected compressed sensing for free-breathing dynamic cardiac MRI},
	Volume = {70},
	Year = {2013},
