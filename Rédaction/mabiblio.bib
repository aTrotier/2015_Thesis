	Month = {Mar},
	Number = {3},
	Pages = {501-16},
	Pmid = {19243031},
	Pst = {ppublish},
	Title = {Undersampled radial MR acquisition and highly constrained back projection (HYPR) reconstruction: potential medical imaging applications in the post-Nyquist era},
	Volume = {29},
