	Pages = {1019-28},
	Pmid = {11922912},
	Pst = {ppublish},
	Title = {Reversal of chronic hypoxia-induced alterations in pulmonary artery smooth muscle electromechanical coupling upon air breathing},
	Volume = {53},
	Year = {2002},
