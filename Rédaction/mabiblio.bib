	Doi = {10.1038/jcbfm.2009.192},
	Journal = {J Cereb Blood Flow Metab},
	Journal-Full = {Journal of cerebral blood flow and metabolism : official journal of the International Society of Cerebral Blood Flow and Metabolism},
	Mesh = {Animals; Blood-Brain Barrier; Central Nervous System Neoplasms; Contrast Media; Electron Spin Resonance Spectroscopy; Ferric Compounds; Humans; Inflammation; Magnetic Resonance Imaging; Nanoparticles; Particle Size},
	Month = {Jan},
