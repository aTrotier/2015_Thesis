	Doi = {10.1186/1532-429X-12-43},
	Journal = {J Cardiovasc Magn Reson},
	Journal-Full = {Journal of cardiovascular magnetic resonance : official journal of the Society for Cardiovascular Magnetic Resonance},
	Mesh = {Animals; Disease Models, Animal; Feasibility Studies; Female; Magnetic Resonance Imaging; Mice; Mice, Inbred C57BL; Myocardial Infarction; Ventricular Function},
	Pages = {43},
	Pmc = {PMC2918602},
	Pmid = {20663156},
	Pst = {epublish},
	Title = {Accelerated cardiovascular magnetic resonance of the mouse heart using self-gated parallel imaging strategies does not compromise accuracy of structural and functional measures},
