	Doi = {10.1186/1532-429X-14-87},
	Journal = {J Cardiovasc Magn Reson},
	Journal-Full = {Journal of cardiovascular magnetic resonance : official journal of the Society for Cardiovascular Magnetic Resonance},
	Mesh = {Adult; Animals; Biomechanical Phenomena; Humans; Magnetic Resonance Imaging, Cine; Mice; Mice, Inbred C57BL; Predictive Value of Tests; Rabbits; Reference Values; Rotation; Species Specificity; Time Factors; Ventricular Function, Left},
	Pages = {87},
	Pmc = {PMC3579745},
	Pmid = {23270566},
	Pst = {epublish},
	Title = {A quantitative comparison of regional myocardial motion in mice, rabbits and humans using in-vivo phase contrast CMR},
