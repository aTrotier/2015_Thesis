	Journal = {Magn Reson Imaging},
	Journal-Full = {Magnetic resonance imaging},
	Mesh = {Adult; Aged; Artifacts; Blood; Cerebral Angiography; Cerebral Arteries; Cerebral Veins; Cerebrovascular Circulation; Female; Hemodynamics; Humans; Image Enhancement; Intracranial Arteriovenous Malformations; Magnetic Resonance Angiography; Male; Middle Aged; Patient Care Planning; Pilot Projects; Radiosurgery; Reproducibility of Results; Steel; Stereotaxic Techniques},
	Number = {3},
	Pages = {227-233},
	Pmid = {8725188},
